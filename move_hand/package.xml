<?xml version="1.0"?>
<package format="2">
  <name>move_hand</name>
  <version>1.0.0</version>
  <description>The move_hand package responsible for moving a robotic prosthesis in gazebo and controlling it correctly.</description>
  <maintainer email="vrisag20@student.aau.dk">Victor Risager</maintainer>
  <maintainer email="ddoros20@student.aau.dk">Daniel Doroshenko</maintainer>
  <maintainer email="jbla20@student.aau.dk">Jakob BÃ¸gh Larsen</maintainer>
